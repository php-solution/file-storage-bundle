    "type": "library",
    "description": "This bundle provides an additional functionality for work with files",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Savchenko",
            "email": "olsav@ciklum.com"
        },
        {
            "name": "Nikita Kalinin",
            "email": "kalinick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "php-solution/doctrine-orm-extra-lib": "^0.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/event-dispatcher": "^3.3",
        "symfony/http-foundation": "^3.3",
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0"
