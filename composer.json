        "psr-4": { "PhpSolution\\FileStorageBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit"
